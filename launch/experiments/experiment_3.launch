<launch>
  <arg name="participent" />
  <arg name="circuit_name" value="sloppy_joystick"/>
  <node name="record" pkg="rosbag" type="record" args="-a -O experimental_results/participent_$(arg participent)/$(arg circuit_name).bag"/>
  <include file="$(find behaviour_circuits)/launch/circuit.launch">
  <arg name="circuit" value="$(arg circuit_name)"/>
  </include> 
</launch>
